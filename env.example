# Copy to .env and fill in values

# Required for WhatsApp via CallMeBot
PHONE=+491570000000
CALLMEBOT_APIKEY=YOUR_CALLMEBOT_APIKEY
# Strip '+' from PHONE for API calls (CallMeBot prefers no plus)
CALLMEBOT_STRIP_PLUS=true

# Files (relative to project root unless absolute)
CSV_FILE=auto_export_finder.csv
DB_FILE=seen_links.db
SEARCH_URLS_FILE=search_urls.txt

# Limits / pacing
MAX_SEND_PER_RUN=8
SLEEP_BETWEEN_SITES=6
HEADLESS=true

# Search filters (ranges)
MIN_YEAR=2019
MAX_YEAR=2025
MIN_KM=0
MAX_KM=150000
MIN_PRICE=5000
MAX_PRICE=25000

# Cron schedule (standard crontab format)
# Default: every 6 hours at minute 0
CRON_SCHEDULE=0 */6 * * *

# Comma-separated lists
# If empty, sensible defaults will be used
CAR_BRANDS_MODELS=BMW 320d,BMW 520d,Mercedes C 220d,Mercedes E 220d,Audi A4,Audi A6,VW Passat,VW Arteon,VW Tiguan,VW Touran,Skoda Superb,Ford Mondeo,Seat Leon,Peugeot 508
